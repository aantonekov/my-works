{
    "type": "object",
    "properties": {
        "login": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50,
            "pattern": "[A-Za-z0-9]{3,50}"
        },
        "firstName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100,
            "pattern": "[A-Za-z0-9]{3,100}"
        },
        "secondName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100,
            "pattern": "[A-Za-z0-9]{3,100}"
        },
        "birthday": {
            "type": "string"
        },
        "age": {
            "type": "string"
        },
        "email": {
            "type": "string",
            "pattern": "^([a-zA-Z0-9]{1,30}\\.[a-zA-Z0-9]{1,30})|([a-zA-Z0-9]{1,30})@[a-z]{1,10}\\.[a-z]{1,5}$",
            "maxLength": 100
        },
        "phone": {
            "type": "string",
            "pattern": "^[0-9]{5,20}$",
            "minLength": 5
        },
        "pwdUser": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100
        },
        "pwdRepeat": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100
        },
        "agreeCheck": {
            "type": "string",
            "minLength": 1,
            "maxLength": 10
        }
    },
    "additionalProperties": false
}